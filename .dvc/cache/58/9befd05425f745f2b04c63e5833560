   "cell_type": "code",
   "execution_count": 165,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "def _str_names(ch_names):\n",
    "    return [str(ch_name) for ch_name in ch_names]\n",
    "\n",
    "def _safe_np_loadtxt(fname, **kwargs):\n",
    "    out = np.genfromtxt(fname, **kwargs)\n",
    "    ch_names = _str_names(out)\n",
    "    others = tuple(out['f%d' % ii] for ii in range(1, len(out.dtype.fields)))\n",
    "    return (ch_names,) + others    "
   ]
  },
